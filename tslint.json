{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "no-var-requires": false,
        "semicolon": [
            true,
            "never"
        ],
        "object-literal-sort-keys": false,
        "variable-name": [
            true,
            "allow-leading-underscore",
            "allow-pascal-case",
            "allow-trailing-underscore"
        ]
    },
    "rulesDirectory": []
}